// Turn heading numbering off on a stylesheet containing the
// "Unnumbered" styles that we used to add in previous versions of UX
// Write. All styles have properties.
core.css.setHeadingNumbering(false);
#item input.css
h1.heading_1 {
    -uxwrite-display-name: "heading 1";
    -uxwrite-next: "p.Normal";
    counter-increment: h1;
    counter-reset: h2 h3 h4 h5 h6;
    font-size: 24pt;
    font-weight: bold;
}

h1.heading_1::before {
    content: counter(h1) " ";
}

h1.heading_1Unnumbered {
    -uxwrite-display-name: "Heading 1 (Unnumbered)";
    -uxwrite-next: "p.Normal";
    -uxwrite-parent: "h1.heading_1";
    counter-increment: h1 0;
    counter-reset: null;
    font-size: 24pt;
    font-weight: bold;
}

h1.heading_1Unnumbered::before, h2.heading_2Unnumbered::before, h3.heading_3Unnumbered::before, h4.heading_4Unnumbered::before, h5.heading_5Unnumbered::before, h6.heading_6Unnumbered::before {
    content: none;
}

h2.heading_2 {
    -uxwrite-display-name: "heading 2";
    -uxwrite-next: "p.Normal";
    counter-increment: h2;
    counter-reset: h3 h4 h5 h6;
    font-size: 18pt;
    font-weight: bold;
}

h2.heading_2::before {
    content: counter(h1) "." counter(h2) " ";
}

h2.heading_2Unnumbered {
    -uxwrite-display-name: "Heading 2 (Unnumbered)";
    -uxwrite-next: "p.Normal";
    -uxwrite-parent: "h2.heading_2";
    counter-increment: h2 0;
    counter-reset: null;
    font-size: 18pt;
    font-weight: bold;
}

h3.heading_3 {
    -uxwrite-display-name: "heading 3";
    -uxwrite-next: "p.Normal";
    counter-increment: h3;
    counter-reset: h4 h5 h6;
    font-size: 14pt;
    font-weight: bold;
}

h3.heading_3::before {
    content: counter(h1) "." counter(h2) "." counter(h3) " ";
}

h3.heading_3Unnumbered {
    -uxwrite-display-name: "Heading 3 (Unnumbered)";
    -uxwrite-next: "p.Normal";
    -uxwrite-parent: "h3.heading_3";
    counter-increment: h3 0;
    counter-reset: null;
    font-size: 14pt;
    font-weight: bold;
}

h4.heading_4 {
    -uxwrite-display-name: "heading 4";
    -uxwrite-next: "p.Normal";
    counter-increment: h4;
    counter-reset: h5 h6;
    font-size: 12pt;
    font-weight: bold;
}

h4.heading_4::before {
    content: counter(h1) "." counter(h2) "." counter(h3) "." counter(h4) " ";
}

h4.heading_4Unnumbered {
    -uxwrite-display-name: "Heading 4 (Unnumbered)";
    -uxwrite-next: "p.Normal";
    -uxwrite-parent: "h4.heading_4";
    counter-increment: h4 0;
    counter-reset: null;
    font-size: 12pt;
    font-weight: bold;
}

h5.heading_5 {
    -uxwrite-display-name: "heading 5";
    -uxwrite-next: "p.Normal";
    counter-increment: h5;
    counter-reset: h6;
    font-size: 10pt;
    font-weight: bold;
}

h5.heading_5::before {
    content: counter(h1) "." counter(h2) "." counter(h3) "." counter(h4) "." counter(h5) " ";
}

h5.heading_5Unnumbered {
    -uxwrite-display-name: "Heading 5 (Unnumbered)";
    -uxwrite-next: "p.Normal";
    -uxwrite-parent: "h5.heading_5";
    counter-increment: h5 0;
    counter-reset: null;
    font-size: 10pt;
    font-weight: bold;
}

h6.heading_6 {
    -uxwrite-display-name: "heading 6";
    -uxwrite-next: "p.Normal";
    counter-increment: h6;
    counter-reset: ;
    font-size: 8pt;
    font-weight: bold;
}

h6.heading_6::before {
    content: counter(h1) "." counter(h2) "." counter(h3) "." counter(h4) "." counter(h5) "." counter(h6) " ";
}

h6.heading_6Unnumbered {
    -uxwrite-display-name: "Heading 6 (Unnumbered)";
    -uxwrite-next: "p.Normal";
    -uxwrite-parent: "h6.heading_6";
    counter-increment: h6 0;
    counter-reset: null;
    font-size: 8pt;
    font-weight: bold;
}
