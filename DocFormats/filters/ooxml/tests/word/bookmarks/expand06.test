ooxml.word.expandBookmarks
#item input.docx
##item document.xml
<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<w:document xmlns:w="http://schemas.openxmlformats.org/wordprocessingml/2006/main">
  <w:body>
    <w:p>
      <w:bookmark w:id="0" w:name="test0">
        <w:bookmark w:id="1" w:name="test1">
          <w:r>
            <w:t>one</w:t>
          </w:r>
        </w:bookmark>
        <w:r>
          <w:t>two</w:t>
        </w:r>
      </w:bookmark>
    </w:p>
  </w:body>
</w:document>
