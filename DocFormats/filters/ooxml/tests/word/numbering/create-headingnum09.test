ooxml.word.create
#item input.html
<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <style>
body {
    counter-reset: h1 h2 h3 h4 h5 h6 figure table;
    margin: 10%;
}

h1 {
    counter-increment: h1;
    counter-reset: h2 h3 h4 h5 h6;
}

h1.Custom1 {
    color: blue;
    text-align: center;
}

h1.Custom2 {
    counter-increment: h1 0;
    counter-reset: null;
}

h1.Custom2::before {
    content: "";
}

h1.Custom3 {
    color: blue;
    text-align: center;
    counter-increment: h1 0;
    counter-reset: null;
}

h1.Custom3::before {
    content: "";
}

h1::before {
    content: counter(h1) " ";
}
  </style>
</head>
<body>
  <h1>One</h1>
  <h1 class="Custom1">Two</h1>
  <h1 class="Custom2">Three</h1>
  <h1 class="Custom3">Four</h1>
</body>
</html>
